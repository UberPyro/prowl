fmod DISTACK is
  sort V S C FN .

  op int : -> V .
  op string : -> V .
  op quote_ : FN -> V [ctor] .
  op list_ : FN -> V [ctor] .

  op val_ : V -> S [ctor] .
  op unit : -> S .
  op _*_ : S S -> S [ctor assoc id:unit] .

  op stack_ : S -> C [ctor] .
  op void : -> C .
  op _+_ : C C -> C [ctor assoc id:void] .

  op fn : C C C -> FN .

endfm
